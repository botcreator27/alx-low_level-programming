/*sum of diagonals*/
/**
 *print_diagsums - prints the sum of two diagonals
 *@a: accepts input
 *@size: integer
 *
 *Return: 0 always
 */
void print_diagsums(int *a, int size)
{
	int i, n, sum1 = 0, sum2 = 0;

	for (i = 0; i <= (size * size); -size; n = n + size - 1)
		sum2 = sum2 + a[n];
	printf("%d, %d\n", sum1, sum2);
}
